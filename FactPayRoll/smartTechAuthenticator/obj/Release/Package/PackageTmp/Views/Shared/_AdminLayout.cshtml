    <script>
        function showMessage(title, message) {
            if (title == "success") {
                $(".toast").css("background -color", "#28b779  !important");
                $(".toast-header ").css("background -color", "#28b779  !important");
            }
            else if (title == "info") {
                $(".toast").css("background -color", "#ffb848  !important");
                $(".toast-header ").css("background -color", "#ffb848  !important");
            }
            else {
                $(".toast").css("background -color", "#da542e  !important");
                $(".toast-header ").css("background -color", "#da542e  !important");
            }

            $(".toast-title").text(title);
            $(".toast-message").text(message);
            var toastElList = [].slice.call(document.querySelectorAll('.toast'))
            var toastList = toastElList.map(function (toastEl) {
                return new bootstrap.Toast(toastEl)
            })
            toastList.forEach(toast => toast.show())
        }


    </script>
    @{
        if (TempData["toastmessage"] != null)
        {
            ToastModel toastModel = (ToastModel)TempData["toastmessage"];
            foreach (ToastMessage toast in toastModel.ToastMessages)
            {
                <script>
                    showMessage('@toast.Title', '@toast.Message')
                </script>
            }
        }
    }

@model smartTechAuthenticator.ViewModels.CheckOutViewModel
@{
    ViewBag.Title = "Payment";
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
}

<link rel="stylesheet" href="~/Content/assets-mall/css/all.min.css">
<link rel="stylesheet" href="~/Content/assets-mall/css/bootstrap.min.css">
<link rel="stylesheet" href="~/Content/assets-mall/css/style.css">
<style>
    .text-end {
        text-align: left !important;
    }

    .card-body:before, .card-body:after {
        content: "";
        display: block;
        clear: both;
    }

    .page-breadcrumb .col-12.d-flex.no-block.align-items-center {
        display: flex !important;
    }

    form.form-horizontal .card-body {
        box-shadow: 0 0 10px rgba(0,0,0,0.3);
        border-radius: 10px;
        margin-top: 20px;
    }
</style>
<div class="page-breadcrumb" style=" margin-top: 90px; margin-left: 88px;">
    <div class="row">
        <div class="col-12 d-flex no-block align-items-center">
            <h4 class="page-title">Payment Detail</h4>
            <div class="ms-auto text-end">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href=@Url.Action("Index","Mall")>Mall</a></li>
                        <li class="breadcrumb-item active" aria-current="page">
                            Payment
                        </li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>
<div class="payment" style="margin-top:30px">
    <div class="pay-inner">
        <div class="cards one">
            <div class="details">
                <p>
                    <i class="fa fa-th" aria-hidden="true"></i>
                    <span>TDLabs</span>
                </p>
                <p>
                    <i class="fa fa-map-marker" aria-hidden="true"></i>
                    <span>@Model.CustomerDetail.Address1 ,@Model.CustomerDetail.Address2 ,@Model.CustomerDetail.PostCode </span>
                </p>
                <p>
                    <i class="fa fa-envelope" aria-hidden="true"></i>
                    <span>@Model.CustomerDetail.Email</span>
                </p>
                <p>
                    <i class="fa fa-phone" aria-hidden="true"></i>
                    <span>@Model.CustomerDetail.MobileNo</span>
                </p>
                <p>
                    <i class="fa fa-globe" aria-hidden="true"></i>
                    <a href="#">https://demo.com</a>
                </p>
            </div>
        </div>
        <div class="time">
            <p>
                Time Remaining : <b>09</b> Minute <b>51</b> Seconds
            </p>
        </div>

        <div class="cards two">
            <div class="pay-details">
                <div class="head">
                    <p>
                        <b>$</b> Payment Details
                    </p>
                </div>
                <div class="body">
                    <p><b>Order ID</b></p>
                    <p>@Model.OrderDetail.Order_Id</p>

                    <p><b>Payment ID</b></p>
                    <p>@Model.OrderDetail.PaymentId</p>

                    <p><b>Payment for</b></p>
                    <p>@Model.OrderDetail.CreatedDate</p>

                    <p><b>Total</b></p>
                    <p><b>MYR @Model.OrderDetail.TotalAmount</b></p>
                </div>
            </div>
        </div>

        <div class="other-pay">
            <h5>Others Payment</h5>
            <div class="accordion" id="payments">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="headingOne">
                        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne">
                            MY (MYR 208.00)
                        </button>
                    </h2>
                    <div id="collapseOne" class="accordion-collapse collapse show" data-bs-parent="#payments">
                        <div class="accordion-body">
                            <div class="accordion" id="payments-options">
                                <div class="accordion-item">
                                    <h2 class="accordion-header" id="subHeadingOne">
                                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#subOptionOne">
                                            Online Banking
                                        </button>
                                    </h2>
                                    <div id="subOptionOne" class="accordion-collapse collapse" data-bs-parent="#payments-options">
                                        <div class="accordion-body">
                                            <a href="#">
                                                <img src="data:image/png;base64," alt="">
                                            </a>
                                            <a href="#">
                                                <img src="https://www.clipartkey.com/mpngs/m/135-1352218_transparent-background-mastercard-logo.png" alt="">
                                            </a>
                                            <a href="#">
                                                <img src="data:image/png;base64," alt="">
                                            </a>
                                        </div>
                                    </div>
                                </div>

                                <div class="accordion-item">
                                    <h2 class="accordion-header" id="subHeadingTwo">
                                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#subOptionTwo">
                                            eWallet
                                        </button>
                                    </h2>
                                    <div id="subOptionTwo" class="accordion-collapse collapse" data-bs-parent="#payments-options">
                                        <div class="accordion-body">
                                            <a href="#">
                                                <img src="https://aniportalimages.s3.amazonaws.com/media/details/pauyau_PlnW2rW.jpg" alt="">
                                            </a>
                                            <a href="#">
                                                <img src="https://lh3.googleusercontent.com/lfSN8-0uxLdHSqBD9ULaZUiBRJ_9lCKK8JqHGWhdgy4WjGJNYQtQ5hPbw2RBCBfEABPTqljEVA4J2J3Pr-emxqnIZu16WIt41CE7Mg" alt="">
                                            </a>
                                            <a href="#">
                                                <img src="data:image/png;base64," alt="">
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </div>
    </div>

    <footer>
        <div class="page-center">
            <p>By submitting the payment, you hereby agree to</p>
            <p>
                <a href="#">eGHL PDPA Policy</a>
            </p>
            <p>
                <a href="#">Cancel and Return to TDLabs</a>
            </p>
        </div>
    </footer>
</div>
<script src="~/Content/assets-mall/js/jquery.min.js"></script>
<script src="~/Content/assets-mall/js/bootstrap.bundle.min.js"></script>
<script src="~/Content/assets-mall/js/main.js"></script>
